C >>>> PARAMETERS: Modify IM,JM,KB,KSL,KSMAX,LAYMAX TO CONFORM TO YOUR
C          model_grid FILE AND run_data FILE
C
       PARAMETER (IM=52,JM=7,KB=11,KSL=10,KSMAX=2,LAYMAX=7) !IDEAL CHANNEL   
C
C >>>> OTHER PARAMETERS
C
      PARAMETER (IMM1=IM-1,JMM1=JM-1,KBM1=KB-1)
      PARAMETER (IMM2=IM-2,JMM2=JM-2,KBM2=KB-2)
      PARAMETER (MAXWET=IM*JM)
C
C  MAX NUMBER OF LAYERS IN CHEM BED MODEL
C
      PARAMETER (NBEDMAX=100)
C
C
      REAL ISPI,ISP2I,KM,KH,KQ,L,NU
      INTEGER EPTS,VPTS,FPTS
      INTEGER IHISTM,ITRACM,EPTSM,VPTSM,FPTSM,EBCM,QBCM,DBCM,PSCM
