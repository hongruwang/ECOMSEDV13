     .    KH(IM,JM,KB),KQ(IM,JM,KB),L(IM,JM,KB),Q2(IM,JM,KB),
     .    Q2B(IM,JM,KB),AAM(IM,JM,KB),Q2L(IM,JM,KB),Q2LB(IM,JM,KB),
