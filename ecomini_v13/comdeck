      REAL*8 ARCTHIK(IM,JM),THSAVE(VPTSM)
C
      REAL NIKURH
C
      REAL*8 SEDMASS(NBEDMAX,IM,JM),CHEMMASS(NBEDMAX,IM,JM),
     +             SEDDEP(IM,JM)
C 
      DATA RHO0/1000./

      COMMON/BLKCON/ISPI,ISP2I,SMOTH,THETA,DAYI,
     .    DTE,DTI,HPRNU,VPRNU,UMOL,GRAV,RAMP,NU,HORCON,ALPHA,TLAG,TIME,
