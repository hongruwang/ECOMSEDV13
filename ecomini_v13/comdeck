     .    T1W,T2W,TX2D(IM,JM),DTX2D(IM,JM,2),TY2D(IM,JM),DTY2D(IM,JM,2),
     .    T1TS,T2TS,TBDRY(EBCM,KBM1),DTBDRY(EBCM,KBM1,2),
     .              SBDRY(EBCM,KBM1),DSBDRY(EBCM,KBM1,2),
     .              TDIF(DBCM,KBM1),SDIF(DBCM,KBM1),
