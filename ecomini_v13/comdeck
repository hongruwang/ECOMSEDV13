     .    DUM(IM,JM),DVM(IM,JM),FSM(IM,JM),COR(IM,JM),CURV42D(IM,JM),
     .    WUSURF(IM,JM),WVSURF(IM,JM),WUBOT(IM,JM),WVBOT(IM,JM),
     .    WTSURF(IM,JM),WSSURF(IM,JM),TPS(IM,JM),AAM2D(IM,JM),
     .    UAF(IM,JM),UA(IM,JM),UAB(IM,JM),VAF(IM,JM),VA(IM,JM),
